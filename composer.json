{
    "name": "hmdana/laravel-inertia-datagrid",
    "description": "best package to make datagrid with inertia.js and laravel with laravel query builder",
    "homepage": "https://github.com/hmdana/laravel-inertia-datagrid",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hussain Mousavi",
            "email": "shm379@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.67|^v9.0.0-beta.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "inertiajs/inertia-laravel": "^0.5",
        "orchestra/testbench": "^6.23|^7.0",
        "protonemedia/inertiajs-tables-laravel-query-builder": "@dev",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Hmdana\\LaravelInertiaDataGrid\\": "php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hmdana\\LaravelInertiaDataGrid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --verbose"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Hmdana\\LaravelInertiaDataGrid\\LaravelInertiaDataGridServiceProvider"
            ]
        }
    }
}
